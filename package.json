{
  "name": "@adopets/ds",
  "license": "MIT",
  "scripts": {
    "dev": "turbo run dev --no-daemon --parallel",
    "build": "turbo run build",
    "build-tokens-react": "npm run  --workspace @adopets/tokens  \"build\" && npm run  --workspace @adopets/design-system  \"build\"  ",
    "clearall": "concurrently \"rm -rf node_modules/\"  \"rm -rf packages/docs/node_modules/\"  \"rm -rf packages/react/node_modules/\" \"rm -rf packages/tokens/node_modules/\" ",
    "clear-dist": "npm run --workspace  @adopets/design-system  clear-dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "github-pages-deploy-action": "^3.5.1",
    "postcss": "^8.4.21",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.5.3",
    "ts-transformer-keys": "^0.4.4",
    "turbo": "^1.7.4"
  },
  "dependencies": {
    "@adopets/design-system": "*",
    "@adopets/tokens": "*"
  }
}